{
  "name": "jsfmt",
  "version": "1.5.0",
  "private": true,
  "description": "Format your JavaScript using jsfmt",
  "scripts": {
    "test": "eslint ."
  },
  "activationCommands": {
    "atom-workspace": [
      "jsfmt:execute"
    ]
  },
  "configSchema": {
    "formatOnSave": {
      "title": "Format on Save",
      "description": "Format JavaScript on Save.",
      "type": "boolean",
      "default": false
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1000ch/atom-jsfmt.git"
  },
  "engines": {
    "atom": ">=1.0.0"
  },
  "keywords": [
    "JavaScript",
    "format",
    "jsfmt"
  ],
  "author": {
    "name": "1000ch",
    "email": "shogo.sensui@gmail.com",
    "url": "http://github.com/1000ch"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1000ch/atom-jsfmt/issues"
  },
  "homepage": "https://github.com/1000ch/atom-jsfmt#readme",
  "dependencies": {
    "execa": "^0.6.0",
    "jsfmt": "^0.5.3"
  },
  "devDependencies": {
    "eslint": "^3.17.1"
  }
}
